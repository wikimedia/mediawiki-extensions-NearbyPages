{
	"opts": {
		"destination": "docs/js",
		"package": "package.json",
		"pedantic": true,
		"recurse": true
	},
	"plugins": [
		"node_modules/jsdoc-vuejs",
		"plugins/markdown"
	],
	"source": {
		"includePattern": "\\.(vue|js)$",
		"include": [ "resources" ]
	},
	"templates": {
		"cleverLinks": true,
		"default": {
			"useLongnameInNav": true
		}
	}
}
